        {
            "name": "match_client_stderr",
            "args": ["crunchy_2", "Client [0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12} logged in with login bastien"]
